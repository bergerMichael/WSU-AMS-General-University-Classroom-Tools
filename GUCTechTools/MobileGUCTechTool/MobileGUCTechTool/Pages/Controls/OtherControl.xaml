<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileGUCTechTool.Pages.Controls.OtherControl"
			 BackgroundColor="#FF2F2F2F">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="&lt;   Other   &gt;" HorizontalTextAlignment="Center" 
			   FontSize="Large" BackgroundColor="#FF2F2F2F"
			   TextColor="White"/>
			<Button x:Name="proj2PwrOn" Text="Projector 2 Power Up" BackgroundColor="Gray" TextColor="White"/>
            <Button x:Name="proj2PwrOff" Text="Projector 2 Power Down" BackgroundColor="Gray" TextColor="White"/>
			<Button x:Name="proj2Mute" Text="Projector 2 Mute" BackgroundColor="Gray" TextColor="White"/>
            <Button x:Name="proj2Unmute" Text="Projector 2 Unmute" BackgroundColor="Gray" TextColor="White"/>
			<Button x:Name="proj2screenUp" Text="Screen 2 Up" BackgroundColor="Gray" TextColor="White"/>
            <Button x:Name="proj2screenDown" Text="Screen 2 Down" BackgroundColor="Gray" TextColor="White"/>
            
		</StackLayout>
    </ContentPage.Content>
</ContentPage>